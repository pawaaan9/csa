{
  "extends": "astro/tsconfigs/strictest",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": [
    "dist",
    "src/pages/autonomous-vehicle-safety.astro",
    "src/pages/fmea-fta-services.astro"
  ],
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    }
  }
}
